<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="simple_workcell">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- a tray for building kits -->
    <include>
      <name>kit_tray_1</name>
      <uri>model://kit_tray</uri>
      <pose>0.6 -1.5 1.27 0 0 0</pose>
    </include>

    <!-- tray 2 for building kits -->
    <include>
      <name>kit_tray_2</name>
      <uri>model://kit_tray</uri>
      <pose>0.6 0.5 1.27 0 0 0</pose>
    </include>

    <!-- Populate kit tray 1 -->
    <plugin filename="libPopulationPlugin.so" name="populate_kit_tray_1">
      <loop_forever>false</loop_forever>
      <frame>kit_tray_1</frame>
      <object_sequence>
        <object>
          <time>2.0</time>
          <type>part1</type>
          <pose>-0.2 -0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>4.0</time>
          <type>part2</type>
          <pose>0. 0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>6.0</time>
          <type>part1</type>
          <pose>-0.2 0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>8.0</time>
          <type>part2</type>
          <pose>-0.2 0.0 0 0 0 0</pose>
        </object>
      </object_sequence>
    </plugin>

    <!-- Populate kit tray 2 -->
    <plugin filename="libPopulationPlugin.so" name="populate_kit_tray_2">
      <loop_forever>false</loop_forever>
      <frame>kit_tray_2</frame>
      <object_sequence>
        <object>
          <time>2.0</time>
          <type>part1</type>
          <pose>-0.2 -0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>4.0</time>
          <type>part2</type>
          <pose>0. 0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>6.0</time>
          <type>part1</type>
          <pose>-0.2 0.15 0 0 0 0</pose>
        </object>
        <object>
          <time>8.0</time>
          <type>part2</type>
          <pose>-0.2 0.0 0 0 0 0</pose>
        </object>
      </object_sequence>
    </plugin>

    <!-- The NIST-ARIAC task manager -->
    <plugin filename="libROSAriacTaskManagerPlugin.so" name="task_manager">
      <robot_namespace>ariac_task_manager</robot_namespace>
      <team_start_service_name>/ariac/start_competition</team_start_service_name>
      <conveyor_activation_topic>~/conveyor/control</conveyor_activation_topic>
      <goals_topic>/ariac/goals</goals_topic>
      <goal>
        <time>0.0</time>
        <kit>
          <tray>kit_tray_1</tray>
          <object>
            <type>part1</type>
            <pose>-0.2 -0.15 0 0 0 0</pose>
          </object>
          <object>
            <type>part2</type>
            <pose>-0.2 0 0 0 0 0</pose>
          </object>
          <object>
            <type>part2</type>
            <pose>-0.2 0.15 0 0 0 0</pose>
          </object>
        </kit>
        <kit>
          <tray>kit_tray_2</tray>
          <object>
            <type>part2</type>
            <pose>-0.2 -0.15 0 0 0 0</pose>
          </object>
          <object>
            <type>part1</type>
            <pose>-0.2 0 0 0 0 0</pose>
          </object>
          <object>
            <type>part2</type>
            <pose>-0.2 0.15 0 0 0 0</pose>
          </object>
        </kit>
      </goal>
    </plugin>

    <!-- The NIST-ARIAC scoring plugin -->
    <plugin filename="libROSAriacScoringPlugin.so" name="scorer">
    </plugin>
  </world>
</sdf>
