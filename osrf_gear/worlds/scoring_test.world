<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="simple_workcell">

    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- a tray for building kits -->
    <include>
      <uri>model://kit_tray</uri>
      <pose>0.6 -1.5 1.27 0 0 0</pose>
    </include>

    <!-- Populate the conveyor belt -->
    <plugin filename="libPopulationPlugin.so" name="populate_conveyor">
      <!--<activation_topic>~/conveyor/control</activation_topic>-->
      <loop_forever>true</loop_forever>
      <frame>kit_tray</frame>
      <object_sequence>
        <object>
          <time>2.0</time>
          <type>coke_can</type>
          <pose>0. 0.0 0 0 0 0</pose>
        </object>
        <object>
          <time>4.0</time>
          <type>cordless_drill</type>
          <pose>0. 0.2 0 0 0 0</pose>
        </object>
        <object>
          <time>6.0</time>
          <type>beer</type>
          <pose>0.2 0.1 0 0 0 0</pose>
        </object>
      </object_sequence>
    </plugin>

    <!-- The NIST-ARIAC task manager -->
    <plugin filename="libROSAriacTaskManagerPlugin.so" name="task_manager">
      <robot_namespace>ariac_task_manager</robot_namespace>
      <team_start_service_name>/ariac/start_competition</team_start_service_name>
      <conveyor_activation_topic>~/conveyor/control</conveyor_activation_topic>
      <goals_topic>/ariac/goals</goals_topic>
      <goal>
        <time>0.0</time>
        <!-- 1st kit -->
        <kit>
          <object>
            <type>coke_can</type>
            <pose>-1 2.5 0 0 0 0</pose>
          </object>
          <object>
            <type>cordless_drill</type>
            <pose>-1 2.5 0 0 0 0</pose>
          </object>
          <object>
            <type>beer</type>
            <pose>-1 2.5 0 0 0 0</pose>
          </object>
        </kit>
        <!-- 2nd kit -->
        <kit>
          <object>
            <type>coke_can</type>
            <pose>0 0 0 0 0 0</pose>
          </object>
          <object>
            <type>coke_can</type>
            <pose>0 0 0 0 0 0</pose>
          </object>
          <object>
            <type>beer</type>
            <pose>0 0 0 0 0 0</pose>
          </object>
        </kit>
      </goal>
    </plugin>

    <!-- The NIST-ARIAC scoring plugin -->
    <plugin filename="libROSAriacScoringPlugin.so" name="scorer">
    </plugin>
  </world>
</sdf>
